    "expo-secure-store": "5.0.1",
    "fuse.js": "3.4.5",
    "jsc-android": "241213.1.0",
    "react": "16.8.6",
    "react-native": "0.60.0-rc.3",
    "react-native-background-fetch": "2.6.0",
    "react-native-calendar-events": "1.7.3",
    "react-native-device-info": "2.1.3",
    "react-native-gesture-handler": "1.3.0",
    "react-native-interactable": "1.0.0",
    "react-native-modal": "11.1.0",
