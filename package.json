    "jetifier": "1.5.1",
    "metro-react-native-babel-preset": "0.54.1",
    "prettier": "1.18.2",
    "tslint": "5.18.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-eslint-rules": "5.4.0",
    "tslint-immutable": "6.0.0",
    "tslint-react": "4.0.0",
    "tslint-react-hooks": "2.1.1",
    "typescript": "3.5.2"
