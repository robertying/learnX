    "typesafe-actions": "5.0.0-2"
  },
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@babel/runtime": "7.4.5",
    "@react-native-community/cli": "2.0.0-rc.3",
    "@types/node": "12.0.10",
